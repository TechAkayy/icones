{
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"lib": ["DOM", "ESNext", "WebWorker"],
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"noUnusedLocals": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["vite/client", "vite-plugin-pages/client", "vite-plugin-pwa/client"],
		"allowJs": true
	},
	"exclude": ["dist", "node_modules"]
}
